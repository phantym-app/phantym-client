{
  "scripts": {
    "start": "npx snowpack dev",
    "format": "npx prettier --write \"src/**/*.{ts,tsx,scss}\"",
    "build": "npx snowpack build && npx rollup -c && rm -r build/_dist_ build/_snowpack && mv build/chunks build/_dist_",
    "serve": "npx serve -s ./build",
    "bds": "npx snowpack build && firebase deploy && npx snowpack dev",
    "test": "npx web-test-runner \"src/**/*.test.{ts,tsx}\"",
    "tdd": "npx web-test-runner \"src/**/*.test.{ts,tsx}\" --watch"
  },
  "dependencies": {
